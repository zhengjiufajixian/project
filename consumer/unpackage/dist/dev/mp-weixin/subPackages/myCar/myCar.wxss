view,
text {
  line-height: 1;
}
.color-DCD9D9 {
  color: #dcd9d9;
}
.color-D1D1D1 {
  color: #d1d1d1;
}
.color-FD8A06 {
  color: #fd8a06;
}
.edit-btn-wrap .edit-btn {
  -webkit-border-radius: 40rpx;
          border-radius: 40rpx;
  background-color: #fd8a06;
}
.edit-btn-wrap .delete-btn {
  -webkit-border-radius: 40rpx;
          border-radius: 40rpx;
  background-color: #d3d3d3;
}
.info-wrap {
  background: #fcf1e5;
  padding: 24rpx 0 24rpx 30rpx;
}
.check-wrap {
  background: #FFFFFF;
  -webkit-border-radius: 50%;
          border-radius: 50%;
  padding: 5rpx;
  border: 2rpx solid #e2e3e4;
}
.checked {
  background: -webkit-linear-gradient(258deg, #fe4e08 0%, #ff890a 100%);
  background: linear-gradient(-168deg, #fe4e08 0%, #ff890a 100%);
}
.list-wrap {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.list-wrap .list-item {
  width: 236rpx;
  line-height: 65rpx;
  font-size: 26rpx;
  -webkit-border-radius: 50rpx;
          border-radius: 50rpx;
  color: #FFFFFF;
  text-align: center;
}
.list-wrap .gray {
  background-color: #cccccc;
}
.list-wrap .blue {
  background-color: #1672FF;
}
.add-wrap {
  background: #1672FF;
  -webkit-border-radius: 20rpx;
          border-radius: 20rpx;
}

