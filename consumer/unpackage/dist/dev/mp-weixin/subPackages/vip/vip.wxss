page {
  background: #fff;
}
.top {
  height: 160rpx;
  background-color: #3D3C3A;
}
.banner {
  height: 250rpx;
  margin-bottom: 160rpx;
}
.banner .bg {
  z-index: -1;
  background-color: #3D3C3A;
  height: 80%;
  width: 100%;
  left: 0;
  top: 0;
}
.color-E6C393 {
  color: #E6C393;
}
.color-DCBA8C {
  color: #DCBA8C;
}
.color-DEBA88 {
  color: #DEBA88;
}
.color-FF0000 {
  color: #FF0000;
}
.list .border-bottom {
  width: 60rpx;
  height: 8rpx;
  background: #DDB88E;
  display: inline-block;
  border-radius: 4rpx;
  position: absolute;
  left: 50%;
  bottom: 0;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
}
.list .list-item {
  border: 1rpx solid #EBCB9C;
  border-radius: 10rpx;
  line-height: 98rpx;
  background-size: 100% 100%;
}
.list .active {
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
}
.btn-wrap {
  border-radius: 10rpx;
  margin: 20rpx auto 20px;
}
.fixed-btn {
  position: fixed;
  bottom: 0;
  width: 100%;
  left: 0;
  border-top: 2rpx solid #dedede;
}

