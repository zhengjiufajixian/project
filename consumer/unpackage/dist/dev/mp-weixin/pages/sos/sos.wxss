.page-section {
  height: 100vh;
}
.wrap {
  width: 586rpx;
}
.btn-item {
  width: 45%;
  text-align: center;
  padding: 12rpx 0;
  -webkit-border-radius: 15rpx;
          border-radius: 15rpx;
  background-color: #1672ff;
  color: #FFFFFF;
}
.gray-btn {
  background-color: #c0d4f3;
}
.white-btn {
  background-color: #FFFFFF;
  color: #1672FF;
}
.sos-btn {
  position: fixed;
  bottom: 18rpx;
  width: 120rpx;
  height: 120rpx;
  -webkit-border-radius: 50%;
          border-radius: 50%;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
  background-position: center;
  background-repeat: no-repeat;
}
.list-wrap {
  position: fixed;
  right: 0;
  top: 0;
}
.fixed-btn {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  height: 52rpx;
  background-color: #1672FF;
  color: #FFFFFF;
  font-size: 28rpx;
  -webkit-box-shadow: 0 0 16rpx 1rpx rgba(22, 114, 255, 0.19);
          box-shadow: 0 0 16rpx 1rpx rgba(22, 114, 255, 0.19);
  line-height: 52rpx;
  -webkit-border-radius: 40rpx 0 0 40rpx;
          border-radius: 40rpx 0 0 40rpx;
  -webkit-box-shadow: 0 10rpx 10rpx 0 rgba(6, 0, 1, 0.27);
          box-shadow: 0 10rpx 10rpx 0 rgba(6, 0, 1, 0.27);
}

