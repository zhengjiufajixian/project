.color-ff8080 {
  color: #ff8080;
}
.u-notice-bar {
  padding: 0 !important;
}
.u-news-item {
  font-size: 22rpx !important;
  color: #666666 !important;
}
.top {
  height: 80rpx;
  background-color: #1672ff;
}
.top .location,
.top .msg {
  color: #fff;
}
.top .msg {
  font-size: 60rpx;
}
.banner {
  height: 306rpx;
}
.banner .bg {
  z-index: -1;
  background-color: #1672ff;
  height: 50%;
  width: 100%;
  left: 0;
  top: 0;
}
.news {
  overflow: hidden;
  margin: 18rpx 30rpx;
  padding: 10rpx 0;
  background-color: #fff;
  -webkit-border-radius: 10rpx;
          border-radius: 10rpx;
}
.news .news-title {
  color: #dd524d;
  font-size: 36rpx;
  line-height: 1.5;
  font-weight: bold;
}
.news .news-list .news-item {
  line-height: 2;
  padding-left: 20rpx;
}
.news .news-list .news-item .red-dot {
  display: inline-block;
  width: 10rpx;
  height: 10rpx;
  -webkit-border-radius: 50%;
          border-radius: 50%;
  background-color: #dd524d;
  position: absolute;
  left: 0;
  top: 50%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
}
.news .arrow {
  line-height: 3.5;
}
.common-header-wrap {
  background-color: #fff;
  padding: 28rpx 20rpx;
  -webkit-border-radius: 10rpx 10rpx 0 0;
          border-radius: 10rpx 10rpx 0 0;
}
.common-header-wrap .title {
  font-weight: bold;
}
.common-header-wrap .sub-title {
  color: #909399;
}
.service-list {
  background-color: #fff;
  padding-bottom: 12rpx;
  -webkit-border-radius: 0 0 10rpx 10rpx;
          border-radius: 0 0 10rpx 10rpx;
}
.service-list .list-item {
  display: inline-block;
  height: 172rpx;
  width: 324rpx;
  background: url(data:image/jpeg;base64,) no-repeat center center;
  background-size: cover;
  padding-left: 16rpx;
  padding-top: 30rpx;
  margin: 0 10rpx 18rpx;
  -webkit-border-radius: 15rpx;
          border-radius: 15rpx;
}
.service-list .list-item .item-title {
  font-weight: bold;
}
.service-list .list-item .item-content {
  line-height: 50rpx;
}
.around-service .list-item {
  background-color: #fff;
  padding-bottom: 20rpx;
}
.around-service .list-item .list-content {
  padding-bottom: 16rpx;
}
.around-service .list-item .list-content .img {
  width: 236rpx;
  height: 164rpx;
  margin: 0 20rpx;
}
.around-service .list-item .list-content .shop-name {
  font-weight: bold;
}
.around-service .list-item .list-content .shop-rate {
  color: #ffba09;
}
.around-service .list-item .list-content .shop-address {
  color: #909399;
  line-height: 3;
  width: 90%;
}
.around-service .list-item .list-content .shop-distance,
.around-service .list-item .list-content .shop-sale {
  color: #c8c9cc;
}
.around-service .list-item .list-content .shop-sale {
  margin-left: 32rpx;
}
.around-service .list-item .list-btn {
  position: absolute;
  right: 20rpx;
  bottom: 36rpx;
}

