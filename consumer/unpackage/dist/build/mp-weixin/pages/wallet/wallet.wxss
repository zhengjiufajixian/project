page{background-color:#fff}.bg-linear{background:-webkit-linear-gradient(56deg,#1672ff,#16baff);background:linear-gradient(34deg,#1672ff,#16baff);-webkit-border-radius:20rpx;border-radius:20rpx}.btn{border:2rpx solid #fff;-webkit-border-radius:10rpx;border-radius:10rpx;padding:6rpx 10rpx}.active{color:#1672ff}.list-item{-webkit-border-radius:10rpx;border-radius:10rpx;-webkit-box-shadow:0 0 24rpx 0 rgba(200,159,206,.49);box-shadow:0 0 24rpx 0 rgba(200,159,206,.49)}.list-item .dot{display:inline-block;width:7rpx;height:7rpx;-webkit-border-radius:50%;border-radius:50%;background:#0036ff;vertical-align:middle}