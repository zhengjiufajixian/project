.u-image {
  margin: 0 auto;
}
.user-info {
  background: url(data:image/png;base64,) no-repeat center center;
  background-size: 100% 100%;
  height: 390rpx;
  color: #fff;
}
.avatar {
  display: inline-block;
  margin-top: 20rpx;
}
.user-name {
  margin: 20rpx 0 10rpx 0;
}
.wrap1,
.wrap2,
.wrap3,
.wrap4 {
  background-color: #fff;
  margin: 18rpx 30rpx 0;
  padding: 25rpx 0 22rpx 0;
  -webkit-border-radius: 15rpx;
          border-radius: 15rpx;
}
.wrap1 .list-item .number {
  text-align: center;
  font-size: 36rpx;
  color: #1672ff;
  font-weight: bold;
}
.wrap1 .list-item .title {
  text-align: center;
  font-size: 26rpx;
  color: #666;
}
.wrap2 {
  padding-left: 30rpx;
  padding-right: 30rpx;
  color: #666;
}
.wrap3 {
  -webkit-border-radius: 15rpx 15rpx 0 0;
          border-radius: 15rpx 15rpx 0 0;
  color: #666;
}
.wrap3 .title {
  font-size: 28rpx;
  font-weight: bold;
  text-indent: 30rpx;
  margin-bottom: 40rpx;
  color: #000;
}
.wrap4 {
  margin: 0rpx 30rpx 0;
  -webkit-border-radius: 0 0 15rpx 15rpx;
          border-radius: 0 0 15rpx 15rpx;
  color: #666;
}
.info-list {
  background-color: #FFFFFF;
}

